#include <iostream>

int main ()
{
    int n, k;
    int a[10000];
    std::cin >> n;
    for (int i=0; i<n; ++i){
        std::cin >> a[i];
    }
    for (int i=0; i<n/2; ++i){
        k=a[i];
        a[i] = a[n-i-1];
        a[n-i-1] = k;
            }
    for (int i=0; i<n; ++i){

        std::cout << a[i] << "  ";
    }

    return 0;
}
