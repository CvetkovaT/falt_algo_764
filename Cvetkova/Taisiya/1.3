#include <iostream>

using namespace std;

int main ()
{
    int a,b,c,d,s,k,t;
    cin >> a >> b >> c >> d;
    k = a*d+b*c;
    s = b*d;
    a = s;
    b = k;
    while (b != 0) {
       t = b;
       b = a % b;
       a = t;
     }
    s /=a;
    k /=a;
    cout << k << " " << s;


    return 0;
}
